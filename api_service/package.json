{
  "name": "api_service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --maxWorkers=1",
    "test:watch": "cross-env NODE_ENV=test jest --watch --maxWorkers=1",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config test/jest-e2e.json --maxWorkers=1",
    "migrate:dev": "yarn prisma migrate dev",
    "db:seed": "yarn prisma db seed",
    "db:migrate": "yarn prisma migrate deploy",
    "db:generate-migration": "yarn prisma migrate dev --create-only",
    "db:reset": "yarn prisma migrate reset",
    "nest:generate": "nest generate"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.3",
    "@prisma/client": "^4.7.0",
    "@types/axios": "^0.14.0",
    "@types/cookie-session": "^2.0.44",
    "@types/ejs": "^3.0.3",
    "@types/multer": "^1.4.7",
    "@types/passport-jwt": "^3.0.7",
    "@types/pug": "2.0.6",
    "amazon-cognito-identity-js": "^6.0.1",
    "aws-sdk": "^2.1309.0",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compose-function": "^3.0.3",
    "cookie-session": "^2.0.0",
    "cross-env": "^7.0.3",
    "ejs": "^3.1.2",
    "email-templates": "^10.0.1",
    "handlebars": "^4.7.6",
    "jwks-rsa": "^3.0.0",
    "nestjs-s3": "^1.0.1",
    "nodemailer": "^6.9.1",
    "openid-client": "^5.4.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "prisma": "^4.7.0",
    "pug": "^3.0.2",
    "randomstring": "^1.2.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sqlite3": "^5.1.2",
    "swagger-ui-express": "^4.6.0",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "jsonwebtoken": "^8.5.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
