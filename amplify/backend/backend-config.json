{
  "auth": {
    "remotesanta": {
      "service": "Cognito",
      "serviceType": "managed",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "frontendAuthConfig": {
        "socialProviders": [],
        "usernameAttributes": [],
        "signupAttributes": ["EMAIL"],
        "passwordProtectionSettings": {
          "passwordPolicyMinLength": 8,
          "passwordPolicyCharacters": []
        },
        "mfaConfiguration": "OFF",
        "mfaTypes": ["SMS"],
        "verificationMechanisms": ["EMAIL"]
      }
    }
  },
  "api": {
    "api_service": {
      "resourceName": "api_service",
      "dependsOn": [
        {
          "category": "",
          "resourceName": "NetworkStack",
          "attributes": [
            "ClusterName",
            "VpcId",
            "VpcCidrBlock",
            "SubnetIds",
            "VpcLinkId",
            "CloudMapNamespaceId"
          ]
        },
        {
          "category": "auth",
          "resourceName": "remotesanta",
          "attributes": ["UserPoolId", "AppClientIDWeb"]
        }
      ],
      "deploymentMechanism": "FULLY_MANAGED",
      "imageSource": {
        "type": "CUSTOM"
      },
      "restrictAccess": true,
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "ElasticContainer",
      "authName": "remotesanta",
      "skipHashing": false,
      "apiType": "REST",
      "iamAccessUnavailable": true
    }
  }
}
